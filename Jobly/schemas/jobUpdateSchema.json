{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"$id": "http://example.com/example.json",
	"type": "object",
	"default": {},
	"title": "Root Schema",
	"required": [],
	"properties": {
		"title": {
			"type": "string",
			"default": "",
			"title": "The title Schema",
			"examples": ["Job Title"]
		},
		"salary": {
			"type": "integer",
			"default": 0,
			"title": "The salary Schema",
			"examples": [10000]
		},
		"equity": {
			"type": "string",
			"default": "0.0",
			"title": "The equity Schema",
			"examples": ["0.2"]
		}
	},
	"examples": [
		{
			"title": "Job Title",
			"salary": 10000,
			"equity": "0.2"
		}
	]
}
